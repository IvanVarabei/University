calcula_source("\nclear\n\nwrite \"Hello, Process up to what number? \"\n\nread MAX_VALUE\n\nnl write \"How many exponents? \"\n\nread NUMBER_OF_EXPONENTS\n\nfor VALUE = 1 to MAX_VALUE do\n	nl\n	for EXPONENT = 1 to NUMBER_OF_EXPONENTS - 1 do\n		write VALUE, \"^\", EXPONENT\n		write \" = \", VALUE^EXPONENT, \", \" nl\n	enddo\n	write VALUE, \"^\", NUMBER_OF_EXPONENTS\n	write \" = \", VALUE^NUMBER_OF_EXPONENTS\nenddo")
