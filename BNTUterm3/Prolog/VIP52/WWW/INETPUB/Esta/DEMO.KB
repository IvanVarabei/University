vars("parameter esta : ''\ntype category\nexplanation 'You can choose to get information about ESTA or to consult a knowledge base.'\noptions\n information - 'Information about ESTA',\n car - 'Knowledge base about cars',\n sanocor - 'Knowledge base about health'.\n/* rules */\nquestion 'Choose a subject to explore.'\npicture 'estademo'","esta","",[c_str("You can choose to get information about ESTA or to consult a knowledge base.",48)],[c_str("Choose a subject to explore.",281)],category,category([option("information","Information about ESTA"),option("car","Knowledge base about cars"),option("sanocor","Knowledge base about health")],[]),"estademo")
